#include <stdio.h>
int main(){
	int n,i,t; // n = 전체 학생 수 , i = 변수, t =  학생의 번호 
	int a[24] = {}; //0부터 23번까지의 배열을 초기화 , a[t]는 배열이며, 불린 횟수 
	scanf("%d", &n); //개수 입력받기 = 출석이 불리는 횟수
	for(int i=1;i<=n;i++){
		scanf("%d", &t);
		a[t] =a[t}+ 1; //e.g. 만약 10번이 불렸다면 a[10]이고 이때 t값이 10, a[t]= a[t]+1의 형태로 구현할 수도 있다. 
	} 
	for(int i=1;i<=23;i++){
		printf("%d", a[i]); // 원래 "%d"로 했다가 계속 틀렸다고 해서 "%d "로 띄어쓰기를 기재하였다.  
	}
	return 0;
}

//#P4C
